{
  "name": "axle",
  "version": "0.1.0",
  "description": "axle is a command line tool for running workflows against LLM APIs.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "ax": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx ./src/cli.ts",
    "build": "pkgroll --clean-dist --minify",
    "build-dev": "pkgroll --clean-dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "get-models": "node scripts/getModels.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0",
    "@commander-js/extra-typings": "^13",
    "chalk": "^5.3.0",
    "commander": "^13",
    "glob": "^11.0.0",
    "openai": "^4.55.7",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "pkgroll": "^2.4.2",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "ts-jest": "^29.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  }
}
